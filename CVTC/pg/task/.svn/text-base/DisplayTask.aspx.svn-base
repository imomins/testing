<%@ Page Language="C#" AutoEventWireup="true" CodeFile="DisplayTask.aspx.cs" Inherits="pg_task_DisplayTask" %>

<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<html xmlns="http://www.w3.org/1999/xhtml">
<head runat="server">
    <title></title>
</head>
<body>
    <form id="form1" runat="server">
    <table style="width: 100%;">
        <tr>
            <td>
              <span style="color:Navy; font-weight:bold;" > Completion Date and Time :</span> &nbsp;&nbsp;<asp:Label ID="LabelCompletionDateTime" runat="server" Text="Label"></asp:Label>
